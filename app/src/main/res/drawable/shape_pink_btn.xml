<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffe5d5ea" />
            <stroke android:width="1dp" android:color="#ffaf7ec1" />
            <corners android:topLeftRadius="8dp" android:topRightRadius="8dp" android:bottomLeftRadius="8dp" android:bottomRightRadius="8dp" />
        </shape>
    </item>
    <item >

        <shape android:shape="rectangle">
            <solid android:color="#fff9f5fa" />
            <stroke android:width="1dp" android:color="#ffaf7ec1" />
            <corners android:topLeftRadius="8dp" android:topRightRadius="8dp" android:bottomLeftRadius="8dp" android:bottomRightRadius="8dp" />
        </shape>
    </item>
</selector>